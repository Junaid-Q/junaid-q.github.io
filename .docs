        <!-- Particles background -->
        <script src="https://cdn.jsdelivr.net/npm/tsparticles@2/tsparticles.bundle.min.js"></script>
        <script>
            function loadParticlesConfig(configFile) {
                const container = document.getElementById('particles-js');
                tsParticles.dom().forEach(instance => instance.destroy());
                while (container.firstChild) container.removeChild(container.firstChild);
                tsParticles.loadJSON('particles-js', configFile);
            }

            document.addEventListener("DOMContentLoaded", () => {
                const configs = ['particles-config.json', 'particles-solid.json'];
                let counter = 0;
                loadParticlesConfig(configs[counter]);
                setInterval(() => {
                    counter = (counter + 1) % configs.length;
                    loadParticlesConfig(configs[counter]);
                }, 5000);
            });

            document.getElementById('fullscreen-btn').addEventListener('click', () => {
                const frame = document.querySelector('.vosviewer-frame');
                if (frame.requestFullscreen) frame.requestFullscreen();
            });
        </script>
