    <script>
        function loadParticlesConfig(configFile) {
                const container = document.getElementById('particles-js');

                // Properly destroy all existing tsParticles instances
                tsParticles.dom().forEach(instance => instance.destroy());

                // Remove old canvases if any remain
                while (container.firstChild) {
                    container.removeChild(container.firstChild);
                }

                // Load the new configuration cleanly
                tsParticles.loadJSON('particles-js', configFile).then(() => {
                    console.log('Loaded:', configFile);
                });
            }

        document.addEventListener("DOMContentLoaded", () => {
            const configs = ['particles-config.json', 'particles-solid.json'];
            let counter = 0;

            loadParticlesConfig(configs[counter]);

            setInterval(() => {
                counter = (counter + 1) % configs.length;
                loadParticlesConfig(configs[counter]);
            }, 5000);
        });
    </script>

